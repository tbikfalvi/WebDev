/* Layout
 ------------ */
* {
	/* Reset browsers default margin, padding and font sizes */
	margin: 0;
	padding: 0;
}

html {
	font-size: 100%;
}

body {
	/* Text-Sizing with ems: http://www.clagnut.com/blog/348/ */
	font-family: "Lucida Grande", Verdana, Helvetica, Arial, sans-serif;
	color: #323D4F;
	background-image: url('./images/bg.gif');
	background-repeat: repeat-xy;
	font-size: 62.5%; /* This sets the default font size to be equivalent to 10px */
	margin: 0;
 background-color: #35361E;
        scrollbar-face-color: #35361E;
        scrollbar-highlight-color: #565737;
        scrollbar-shadow-color: #000000;
        scrollbar-3dlight-color: #565737;
        scrollbar-arrow-color:  #D5D1BB;
        scrollbar-track-color: #42432B;
        scrollbar-darkshadow-color: #958B67;
        margin-left : 20px;
        margin-right : 0px;
        margin-top : 0px;
        margin-bottom : 20px;	
	
	
}

.maintable {
        BORDER-right: #5A5C35 1px solid;
         BORDER-BOTTOM: #5A5C35 1px solid;
         BORDER-left: #5A5C35 1px solid;
         padding: 0px 0px 0px 0px  ;
}

.bodyline        { background-color: #474829 ; border: 0px  solid; }

#wrapheader {
	min-height: 100px;
	height: auto !important;
